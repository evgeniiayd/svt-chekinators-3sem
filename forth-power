<body>
<h4>1140103</h4>
<input type="number" id="NNN">
<button id="bt">Счёт</button>
<br><br>
<input type="text" id="result" readonly>

<script>
document.getElementById('bt').addEventListener('click', function() {
var inputNumber = document.getElementById('NNN').value;
var resultNumber = document.getElementById('result');

// Рассчёт четвёртой степени
var number = parseInt(inputNumber);
var power = Math.pow(number + 1, 4);

// Отображение результата
resultNumber.value = power;
});
</script>
</body>
